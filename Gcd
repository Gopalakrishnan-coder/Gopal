package Day4;
import java.util.Scanner;
public class Gcd {
	public static void main(String[] args) {
		Scanner sc=new Scanner(System.in);
		int x=sc.nextInt();
		int y=sc.nextInt();
		int min=(x<y)?x:y;
		int gcd=0;
		for(int i=min; i>0; i--)
		{
			if(x%i== 0 && y%i == 0)
			{
				gcd =i;
				break;
			}
		}
		System.out.println(gcd);
	}
}


