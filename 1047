package Day5;
import java.util.Scanner;
public class Day5ym1047 {
	public static void main(String[] args) {
		        Scanner sc = new Scanner(System.in);
		        int N = sc.nextInt();
		        int count = 0;
		        int divisor = 5;

		        while (N / divisor > 0) {
		            count += N / divisor;
		            divisor *= 5;
		        }

		        System.out.println(count);
		    }
		

	}


