package Day5;
import java.util.Scanner;
public class Day51024 {
		    public static void main(String[] args) {
		        Scanner sc = new Scanner(System.in);
		        int num = sc.nextInt();
		        System.out.println((num % 2 == 0) ? "Behold, an Even Enigma!" : "Discover the Odd Symphony!");
		    }
		


	}


