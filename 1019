package Day5;
import java.util.Scanner;
public class Day51019 {
	public static void main(String[] args) {
		        Scanner scanner = new Scanner(System.in);
		        int number = scanner.nextInt();
		        int tens = number / 10;
		        int ones = number % 10;
		        int sum = tens + ones;
		        System.out.println(sum);
		    }
		
	}

